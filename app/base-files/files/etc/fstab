none   /tmp       tmpfs      defaults,noatime,nosuid,nodev,mode=1777,size=64M 0 0
none   /run       tmpfs      defaults,noatime,nosuid,nodev,mode=1777,size=4M 0 0
none   /proc      proc       defaults 0 0
none   /dev/pts   devpts     defaults 0 0
none   /sys       sysfs      defaults 0 0
nodev /sys/kernel/debug	   debugfs   defaults   0  0

